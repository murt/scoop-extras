{
    "version": "1.0.117",
    "description": "A terminal for a more modern age",
    "homepage": "https://eugeny.github.io/terminus/",
    "license": "MIT",
    "architecture": {
        "64bit": {
            "url": "https://github.com/Eugeny/terminus/releases/download/v1.0.117/terminus-1.0.117-setup.exe#/dl.7z",
            "hash": "sha512:e52f4e232296a4f67d5aace2891a1af955142d20d197b1a99d1210814e935d603ebad444e18d7d3dda3c5546f57e098f230f21743b5208e07b0ef5f23b07d323",
            "installer": {
                "script": [
                    "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\"",
                    "Remove-Item \"$dir\\`$PLUGINSDIR\", \"$dir\\Unins*\" -Force -Recurse"
                ]
            }
        }
    },
    "bin": "Terminus.exe",
    "shortcuts": [
        [
            "Terminus.exe",
            "Terminus"
        ]
    ],
    "checkver": {
        "github": "https://github.com/Eugeny/terminus"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/Eugeny/terminus/releases/download/v$version/terminus-$version-setup.exe#/dl.7z",
                "hash": {
                    "url": "$baseurl/latest.yml",
                    "regex": "sha512:\\s+$base64"
                }
            }
        }
    }
}
